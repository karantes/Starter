<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="common-clean" template="/WEB-INF/layout/classic-clean.jsp">
		<put-list-attribute name="cssVendors" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/vendors/css/vendors.min.css" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/css/forms/icheck/icheck.css" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/css/forms/icheck/custom.css" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/css/extensions/sweetalert2.min.css" />
		</put-list-attribute>
		<put-list-attribute name="cssTheme" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/css/bootstrap.min.css" />
			<add-attribute value="/resources/stack-admin/app-assets/css/bootstrap-extended.min.css" />
			<add-attribute value="/resources/stack-admin/app-assets/css/colors.min.css" />
			<add-attribute value="/resources/stack-admin/app-assets/css/components.min.css" />
		</put-list-attribute>
		<put-list-attribute name="cssPage" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/css/core/menu/menu-types/horizontal-menu.css" />
			<add-attribute value="/resources/stack-admin/app-assets/css/core/colors/palette-gradient.css" />
		</put-list-attribute>
		<put-list-attribute name="cssCustom" inherit="true">
			<add-attribute value="/resources/stack-admin/assets/css/style.css" />
		</put-list-attribute>

		<put-list-attribute name="jsVendors" inherit="true">
			<add-attribute value="/resources/stack-admin/jquery.min.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/vendors.min.js" />
			<add-attribute value="/resources/stack-admin/jquery-ui.js" />
			<add-attribute value="/resources/stack-admin/jquery.maskMoney.min.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/extensions/sweetalert2.all.min.js" />
		</put-list-attribute>
		<put-list-attribute name="jsPageVendors" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/ui/jquery.sticky.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/charts/jquery.sparkline.min.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/forms/validation/jqBootstrapValidation.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/forms/icheck/icheck.min.js" />
		</put-list-attribute>
		<put-list-attribute name="jsTheme" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/js/core/app-menu.js" />
			<add-attribute value="/resources/stack-admin/app-assets/js/core/app.js" />
		</put-list-attribute>
		<put-list-attribute name="jsPage" inherit="true">
			<add-attribute value="/resources/stack-admin/jquery-migrate-3.0.0.min.js" />
		</put-list-attribute>
	</definition>

	<definition name="common" template="/WEB-INF/layout/classic.jsp">
		<put-attribute name="header" value="/WEB-INF/layout/header.jsp" />
		<put-attribute name="menu" value="/WEB-INF/layout/menu.jsp" />
		<put-attribute name="footer" value="/WEB-INF/layout/footer.jsp" />
		<put-list-attribute name="cssVendors" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/vendors/css/vendors.min.css" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/css/forms/icheck/icheck.css" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/css/forms/icheck/custom.css" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/css/extensions/sweetalert2.min.css" />
		</put-list-attribute>
		<put-list-attribute name="cssTheme" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/css/bootstrap.min.css" />
			<add-attribute value="/resources/stack-admin/app-assets/css/bootstrap-extended.min.css" />
			<add-attribute value="/resources/stack-admin/app-assets/css/colors.min.css" />
			<add-attribute value="/resources/stack-admin/app-assets/css/components.min.css" />
		</put-list-attribute>
		<put-list-attribute name="cssPage" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/css/core/menu/menu-types/horizontal-menu.css" />
			<add-attribute value="/resources/stack-admin/app-assets/css/core/colors/palette-gradient.css" />
		</put-list-attribute>
		<put-list-attribute name="cssCustom" inherit="true">
			<add-attribute value="/resources/stack-admin/assets/css/style.css" />
		</put-list-attribute>

		<put-list-attribute name="jsVendors" inherit="true">
			<add-attribute value="/resources/stack-admin/jquery.min.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/vendors.min.js" />
			<add-attribute value="/resources/stack-admin/jquery-ui.js" />
			<add-attribute value="/resources/stack-admin/jquery.maskMoney.min.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/extensions/sweetalert2.all.min.js" />
		</put-list-attribute>
		<put-list-attribute name="jsPageVendors" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/ui/jquery.sticky.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/charts/jquery.sparkline.min.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/forms/validation/jqBootstrapValidation.js" />
			<add-attribute value="/resources/stack-admin/app-assets/vendors/js/forms/icheck/icheck.min.js" />
		</put-list-attribute>
		<put-list-attribute name="jsTheme" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/js/core/app-menu.js" />
			<add-attribute value="/resources/stack-admin/app-assets/js/core/app.js" />
		</put-list-attribute>
		<put-list-attribute name="jsPage" inherit="true">
			<add-attribute value="/resources/stack-admin/jquery-migrate-3.0.0.min.js" />
		</put-list-attribute>
	</definition>

	<definition name="error-400" extends="common-clean">
		<put-attribute name="title" value="error-400" />
		<put-attribute name="body" value="/WEB-INF/jsp/admin/error-400.jsp" />
	</definition>

	<definition name="error-403" extends="common-clean">
		<put-attribute name="title" value="error-403" />
		<put-attribute name="body" value="/WEB-INF/jsp/admin/error-403.jsp" />
	</definition>

	<definition name="error-404" extends="common-clean">
		<put-attribute name="title" value="error-404" />
		<put-attribute name="body" value="/WEB-INF/jsp/admin/error-404.jsp" />
	</definition>

	<definition name="login" extends="common-clean">
		<put-attribute name="title" value="Login" />
		<put-attribute name="body" value="/WEB-INF/jsp/admin/login.jsp" />
		<put-list-attribute name="cssPage" inherit="true">
			<add-attribute value="/resources/stack-admin/app-assets/css/pages/login-register.css" />
		</put-list-attribute>
		<put-list-attribute name="jsPage" inherit="true">
			<add-attribute value="/resources/js/admin/login.js" />
		</put-list-attribute>
	</definition>

	<definition name="home" extends="common">
		<put-attribute name="body" value="/WEB-INF/jsp/home.jsp" />
		<put-attribute name="title" value="Home" />
	</definition>

</tiles-definitions>